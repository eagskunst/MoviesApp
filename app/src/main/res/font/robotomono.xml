<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <font
        android:font="@font/robotomono_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/robotomono_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        />

    <font
        android:font="@font/robotomono_italic"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/robotomono_italic"
        app:fontStyle="normal"
        app:fontWeight="400"
        />


    <font
        android:font="@font/robotomono_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/robotomono_regular"
        app:fontStyle="normal"
        app:fontWeight="700"
        />

</font-family>